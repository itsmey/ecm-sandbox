<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <File name="cliActionsHistory" fileName="logs/history.log">
            <PatternLayout>
                <Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
            </PatternLayout>
        </File>
    </Appenders>
    <Loggers>
        <Logger name="ru.imikryakov.ecm.cli.Launcher" level="trace" additivity="false">
            <AppenderRef ref="cliActionsHistory"/>
        </Logger>
        <root level="info">
        </root>
    </Loggers>
</Configuration>

